@Article{studienvergleichbayernrheinland,
  author    = {Kißgen, R. and Wirts, C. and Limburg, D. and Wertfein, M. and Franke, S. and Wölfl, J. and Austermühle, J.},
  journal   = {Frühförderung interdisziplinär},
