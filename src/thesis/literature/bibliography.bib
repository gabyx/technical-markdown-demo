@Article{rafferty&Griffin2005,
  author   = {Yvonne Rafferty and Kenneth W. Griffin},
  journal  = {Journal of Early Intervention},
  title    = {Benefits and Risks of Reverse Inclusion for Preschoolers with and without Disabilities: Perspectives of Parents and Providers},
