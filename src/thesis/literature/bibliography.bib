@Article{FroehlichGildhoff2020,
  author    = {Fröhlich-Gildhoff, Klaus and Grasy-Tinius, Claudia and Hoffer, Rieke},
  journal   = {Frühe Bildung},
  title     = {Gelingensfaktoren der Kompetenzentwicklung frühpädagogischer Fachkräfte im Umgang mit herausforderndem Verhalten in der Kindertageseinrichtung.},
